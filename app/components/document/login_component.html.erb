<div data-file-auth-target="loginPanel" hidden>
    <div class="authLinkWrapper">
        <%= render Icons::LockClockComponent.new %>
        <p data-file-auth-target="loginMessage" class="loginMessage"><%= I18n.t('restrictions.stanford_only') %></p>
        <button data-file-auth-target="loginButton" data-action="file-auth#login"
                 data-file-auth-messageId-param="" data-file-auth-url-param="">
                Log in
        </button>
    </div>
</div>
<div data-file-auth-target="messagePanel" hidden>
    <div class="authLinkWrapper">
        <svg class="MuiSvgIcon-root LockOpenSharp" focusable="false" viewBox="0 0 24 24" aria-hidden="true">
            <path d="M20 8h-3V6.21c0-2.61-1.91-4.94-4.51-5.19C9.51.74 7 3.08 7 6h2c0-1.13.6-2.24 1.64-2.7C12.85 2.31 15 3.9 15 6v2H4v14h16zm-2 12H6V10h12zm-6-3c1.1 0 2-.9 2-2s-.9-2-2-2-2 .9-2 2 .9 2 2 2"></path>
        </svg>
        <p class="loginMessage"><%= I18n.t('restrictions.logged_in') %></p>
       <button class="dismissButton" data-action="file-auth#hideMessagePanel" aria-label="Close access message">
         <svg class="MuiSvgIcon-root CloseSharp" focusable="false" aria-hidden="true" viewBox="0 0 24 24">
            <path d="M19 6.41 17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"></path>
        </svg>
        </button>
    </div>
</div>
